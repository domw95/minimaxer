{
    "entryPoints": [
        "src/index.ts"
    ],
    "entryPointStrategy": "expand",
    "out": "docs",
    "includes": "docs-src",
    "includeVersion": true,
    "readme": "docs-src/index.md",
    "gitRevision": "main",
    "searchInComments": true,
    "sort": [
        "enum-value-ascending"
    ],
    "navigationLinks": {
        "GitHub": "https://github.com/domw95/minimaxer",
        "Blog": "https://domwil.co.uk/minimaxer/"
    },
    "validation": {
        "invalidLink": true,
        "notDocumented": true,
        "notExported": true
    },
    "requiredToBeDocumented": [
        "Enum",
        "EnumMember",
        "Variable",
        "Function",
        "Class",
        "Interface",
        "Property",
        "Method",
        "Accessor",
        "TypeAlias"
    ],
    "theme": "default",
    //     "plugin": ["typedoc-theme-hierarchy"],
    //     "theme": "hierarchy"
    // "plugin": ["typedoc-neo-theme"],
    // "theme": "./node_modules/typedoc-neo-theme/bin/default",
}